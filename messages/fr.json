{
  "$schema": "https://inlang.com/schema/inlang-message-format",
  "title": "CV d'Eddie Vallier, développeur fullstack",
  "hello_world": "Hello, {name} from fr!",
  "cv_email": "Email",
  "cv_phone": "Téléphone",
  "cv_address": "Adresse",
  "cv_about": "À propos",
  "cv_section_experiences": "Expériences",
  "cv_section_education": "Études & diplômes",
  "cv_section_competences": "Competences",
  "cv_section_hobbies": "Centres d'intérêt",
  "cv_present": "Présent",
  "cv_obtained_in": "Obtenu en {year}",

  "cv_duration_month": [
    {
      "declarations": ["input months", "local monthsPlural = months: plural"],
      "selectors": ["monthsPlural"],
      "match": {
        "monthsPlural=one": "1 mois",
        "monthsPlural=other": "{months} mois"
      }
    }
  ],

  "cv_duration_year": [
    {
      "declarations": ["input years", "local yearsPlural = years: plural"],
      "selectors": ["yearsPlural"],
      "match": {
        "yearsPlural=one": "1 an",
        "yearsPlural=other": "{years} ans"
      }
    }
  ],

  "cv_duration_year_month": [
    {
      "declarations": ["input years", "input months", "local yearsPlural = years: plural", "local monthsPlural = months: plural"],
      "selectors": ["yearsPlural", "monthsPlural"],
      "match": {
        "yearsPlural=one, monthsPlural=one": "1 an et 1 mois",
        "yearsPlural=one, monthsPlural=other": "1 an et {months} mois",
        "yearsPlural=other, monthsPlural=one": "{years} ans et 1 mois",
        "yearsPlural=other, monthsPlural=other": "{years} ans et {months} mois"
      }
    }
  ]
}
