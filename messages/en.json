{
  "$schema": "https://inlang.com/schema/inlang-message-format",
  "title": "Eddie Vallier's fullstack developer CV",
  "hello_world": "Hello, {name} from en!",
  "cv_email": "Email",
  "cv_phone": "Phone",
  "cv_address": "Address",
  "cv_about": "About",
  "cv_section_experiences": "Experience",
  "cv_section_education": "Education",
  "cv_section_competences": "Competences",
  "cv_section_hobbies": "Hobbies",
  "cv_present": "Present",
  "cv_obtained_in": "Obtained in {year}",
  "cv_duration": [
    {
      "declarations": ["input years", "input months", "local yearsPlural = years: plural", "local monthsPlural = months: plural"],
      "selectors": ["yearsPlural", "monthsPlural"],
      "match": {
        "years=0, monthsPlural=one": "1 month",
        "years=0, monthsPlural=other": "{months} months",
        "yearsPlural=one, months=0": "1 year",
        "yearsPlural=one, monthsPlural=one": "1 year and 1 month",
        "yearsPlural=one, monthsPlural=other": "1 year and {months} months",
        "yearsPlural=other, months=0": "{years} years",
        "yearsPlural=other, monthsPlural=one": "{years} years and 1 month",
        "yearsPlural=other, monthsPlural=other": "{years} years and {months} months"
      }
    }
  ]
}
